{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "unibest",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue",
      "^pf-(.*)": "@/components/pf-$1/pf-$1.vue"
    }
  },
  "tabBar": {
    "color": "#999",
    "selectedColor": "#018d71",
    "backgroundColor": "#F8F8F8",
    "borderStyle": "black",
    "height": "50px",
    "fontSize": "10px",
    "iconWidth": "24px",
    "spacing": "3px",
    "list": [
      {
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/homeHL.png",
        "pagePath": "pages/index/index",
        "text": "首页"
      },
      {
        "iconPath": "static/tabbar/example.png",
        "selectedIconPath": "static/tabbar/exampleHL.png",
        "pagePath": "pages/finance-center/finance-center",
        "text": "融资中心"
      },
      {
        "iconPath": "static/tabbar/example.png",
        "selectedIconPath": "static/tabbar/exampleHL.png",
        "pagePath": "pages/policy/policy",
        "text": "政策中心"
      },
      {
        "iconPath": "static/tabbar/example.png",
        "selectedIconPath": "static/tabbar/exampleHL.png",
        "pagePath": "pages/mine/mine",
        "text": "我的"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "layout": "default",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "pages/finance/finance",
      "type": "page",
      "layout": "finance",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/finance-center/finance-center",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "融资中心"
      }
    },
    {
      "path": "pages/login/login",
      "type": "page",
      "layout": "login",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/mine/mine",
      "type": "page",
      "layout": "finance",
      "style": {
        "navigationBarTitleText": "个人中心",
        "navigationStyle": "custom"
      },
      "needLogin": true,
      "isTab": true
    },
    {
      "path": "pages/other-login/other-login",
      "type": "page",
      "layout": "login",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "登录"
      }
    },
    {
      "path": "pages/policy/policy",
      "type": "page",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "政策中心"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-sub",
      "pages": [
        {
          "path": "assetDataTable/assetDataTable",
          "type": "page",
          "layout": "assetDataTable",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "资产数据入表"
          }
        },
        {
          "path": "enterpriseVerify/enterpriseVerify",
          "type": "page",
          "layout": "authentication",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "企业认证"
          },
          "bgType": 2,
          "needLogin": true
        },
        {
          "path": "faceAuthentication/faceAuthentication",
          "type": "page",
          "layout": "common",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "实名认证",
            "navigationBarBackgroundColor": "#FFFFFF",
            "backgroundColor": "#FFFFFF"
          }
        },
        {
          "path": "policyDetails/policyDetails",
          "type": "page",
          "layout": "common",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "政策详情",
            "navigationBarBackgroundColor": "#FFFFFF",
            "backgroundColor": "#FFFFFF"
          }
        },
        {
          "path": "productApplication/productApplication",
          "type": "page",
          "layout": "authentication",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "产品申请"
          },
          "bgType": 3
        },
        {
          "path": "productDetail/productDetail",
          "type": "page",
          "layout": "searchNavbar",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "产品详情"
          }
        },
        {
          "path": "requirementList/requirementList",
          "type": "page",
          "layout": "common",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "需求列表",
            "navigationBarBackgroundColor": "#F5F5F5",
            "backgroundColor": "#F5F5F5"
          }
        },
        {
          "path": "search/search",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "搜索"
          }
        },
        {
          "path": "searchInput/searchInput",
          "type": "page",
          "layout": "common",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "上一级标题",
            "navigationBarBackgroundColor": "#FFFFFF",
            "backgroundColor": "#FFFFFF"
          }
        },
        {
          "path": "setting/setting",
          "type": "page",
          "layout": "common",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "账号设置",
            "navigationBarBackgroundColor": "#F5F5F5",
            "backgroundColor": "#F5F5F5"
          },
          "needLogin": true
        },
        {
          "path": "smartMatching/smartMatching",
          "type": "page",
          "layout": "common",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "智能匹配",
            "navigationBarBackgroundColor": "#FFFFFF",
            "backgroundColor": "#FFFFFF"
          }
        },
        {
          "path": "supplyTrade/supplyTrade",
          "type": "page",
          "layout": "assetDataTable",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "供应链贸易"
          }
        },
        {
          "path": "userVerify/userVerify",
          "type": "page",
          "layout": "authentication",
          "style": {
            "navigationStyle": "custom",
            "navigationBarTitleText": "实名认证"
          },
          "bgType": 1,
          "needLogin": true
        }
      ]
    }
  ]
}